{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:app": "nest build app",
    "build:payments": "nest build payments",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=DEVELOPMENT nest start --watch",
    "start:app/debug": "cross-env NODE_ENV=DEVELOPMENT nest start app --debug --watch",
    "start:payments/debug": "cross-env NODE_ENV=DEVELOPMENT nest start payments --debug --watch",
    "start:files/debug": "cross-env NODE_ENV=DEVELOPMENT nest start files --debug --watch",
    "start:debug": "cross-env NODE_ENV=DEVELOPMENT nest start --debug --watch",
    "start:prod": "node dist/apps/app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=TEST jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .test.env -- cross-env NODE_ENV=test jest --config ./apps/app/test/jest-e2e.json --runInBand",
    "test/payments:e2e": "cross-env NODE_ENV=TEST  jest --config ./apps/payments/test/jest-e2e.json",
    "start:payments": "node dist/apps/payments/main",
    "start:app": "cross-env NODE_ENV=PRODUCTION node dist/apps/app/main",
    "migrate:app": "dotenv -e .development.env -- npx prisma migrate dev --name add_url_to_profile --schema=./apps/app/prisma/schema.prisma",
    "migrate:app-testing": "dotenv -e .test.env -- npx prisma migrate dev --name add_url_to_profile --schema=./apps/app/prisma/schema.prisma",
    "generate:app": "npx prisma generate --schema=./apps/app/prisma/schema.prisma",
    "prisma:rest": "dotenv -e .development.env -- npx prisma migrate reset --schema=./apps/app/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.651.1",
    "@aws-sdk/lib-storage": "^3.654.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "5.19.1",
    "@types/busboy": "^1.5.4",
    "@types/compression": "^1.7.5",
    "@types/sharp": "^0.32.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "busboy": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "handlebars": "^4.7.8",
    "mongoose": "^8.6.2",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.13",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-github": "^1.1.12",
    "@types/passport-google-oauth2": "^0.1.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.19.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".+(spec|test)\\.(t|j)s$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "packageManager": "pnpm@9.1.0"
}
